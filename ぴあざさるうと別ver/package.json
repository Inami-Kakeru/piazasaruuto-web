    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "node-fetch": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Inami-Kakeru/piazasaruuto.git"
  },
  "homepage": "https://github.com/Inami-Kakeru/piazasaruuto#readme",
